{
  "browsers": ["ie >= 10"],
  "paths": {
    "tokens": {
      "bundle": "cash"
    },
    "output": {
      "javascript": {
        "unminified": "[dist]/[bundle].js",
        "minified": "[dist]/[bundle].min.js"
      }
    }
  },
  "plugins": {
    "wrapper": {
      "options": {
        "templates": {
          "default": "/* MIT https://github.com/kenwheeler/cash */\n(function(){\n[content]\n})();"
        }
      }
    }
  },
  "environments": {
    "production": {
      "plugins": {
        "uglify": {
          "enabled": false
        },
        "closure": {
          "enabled": true,
          "options": {
            "outputWrapper": "",
            "compilationLevel": "SIMPLE"
          }
        }
      }
    }
  }
}

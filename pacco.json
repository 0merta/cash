{
  "browsers": ["ie >= 10"],
  "paths": {
    "tokens": {
      "bundle": "cash"
    },
    "output": {
      "javascript": {
        "unminified": "[dist]/[bundle].js",
        "minified": "[dist]/[bundle].min.js"
      }
    }
  },
  "plugins": {
    "closure": {
      "enabled": true,
      "options": {
        "outputWrapper": "/* MIT https://github.com/kenwheeler/cash */\n(function(){%output%})();"
      }
    },
    "uglify": {
      "enabled": false
    }
  }
}

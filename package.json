{
  "name": "cash-dom",
  "version": "1.2.0",
  "description": "An absurdly small jQuery alternative for modern browsers.",
  "main": "./dist/cash.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenwheeler/cash.git"
  },
  "author": "Ken Wheeler <ken_wheeler@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenwheeler/cash/issues"
  },
  "homepage": "https://github.com/kenwheeler/cash",
  "scripts": {
    "build": "npm run concat && npm run babel && npm run banner && npm run uglify",
    "banner": "headr dist/cash-concat.js -o=dist/cash-concat.js --version --homepage --license",
    "postbuild": "npm run test",
    "babel": "babel dist/cash-concat.js --out-file dist/cash-concat.js",
    "concat": "concat-cli -f src/_wrap-start.js src/core.js src/util.js src/data.js src/attributes.js src/collection.js src/css.js src/dimensions.js src/events.js src/forms.js src/manipulation.js src/offset.js src/traversal.js src/_wrap-end.js -o dist/cash-concat.js",
    "uglify": "uglifyjs --compress --mangle -r cash --screw-ie8 dist/cash-concat.js -o dist/cash-concat.min.js",
    "test": "karma start --single-run"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "concat-cli": "^4.0.0",
    "headr": "^0.1.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.1",
    "karma-opera-launcher": "^0.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-should": "^1.0.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "should": "^8.3.0",
    "uglify-js": "^2.6.2"
  },
  "dependencies": {}
}
